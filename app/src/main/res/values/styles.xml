<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="QMUI.Compat">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="qmui_dialog_action_button_height">42dp</item>
        <item name="qmui_dialog_action_block_btn_height">42dp</item>
        <item name="qmui_dialog_action_button_min_width">68dp</item>
    </style>

    <style name="MyAppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/myColorBg</item>
        <item name="colorPrimaryDark">@color/myColorBg</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

</resources>
